SELECT * FROM BOOK;

 CREATE TABLE LIBRARY_BRANCH(
  BRANCH_ID INT PRIMARY KEY,
  BRANCH_NAME VARCHAR2(20),
  ADDRESS VARCHAR2(20)
 );

